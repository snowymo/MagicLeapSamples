<manifest
	xmlns:ml="magicleap"
	ml:package="com.magicleap.capi.sample.bluetooth"
	ml:version_code="1"
	ml:version_name="1.0">
	<application
		ml:visible_name="bluetooth"
		ml:sdk_version="1.0"
		ml:min_api_level="1">
		<component
			ml:name="bluetooth"
			ml:visible_name="bluetooth"
			ml:binary_name="bin/bluetooth"
			ml:type="Fullscreen">
			<icon ml:model_folder="Empty" ml:portal_folder="Empty" />
		</component>
		<uses-privilege ml:name="LowLatencyLightwear"/>
		<uses-privilege ml:name="BluetoothAdapterUser"/>
		<uses-privilege ml:name="BluetoothAdapterExternalApp"/>
		<uses-privilege ml:name="BluetoothGattWrite"/>
	</application>
</manifest>
